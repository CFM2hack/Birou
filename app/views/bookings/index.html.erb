
<div class="banner text-center" style="background-image: linear-gradient(rgba(0,0,0,0.2), rgba(0,0,0,0.2)), url('<%= image_path 'office2.png' %>');">
  <h2 class="h2a">
    <p><span id="banner-typed-text"></span></p>
  </h2>
  <div class="row-bookings">
    <% @bookings.each do |booking| %>
      <div class="container">
        <div class="card">
          <h5 class="h5a"><%= booking.office.name %></h5>
          <img src="https://dinksystudio.com/wp-content/uploads/2019/09/ligo_klatka4-1024x551.jpg" alt="">
          <p>Start Date:<strong><%= booking.start_date %></strong></p>
          <p>End Date:<strong><%= booking.end_date %></strong></p>
          <p><strong><%= booking.category %></strong></p>
           <a href="#" class="btn btn-flat">Edit</a>
        </div>
      </div>
    <% end %>
  </div>
</div>
